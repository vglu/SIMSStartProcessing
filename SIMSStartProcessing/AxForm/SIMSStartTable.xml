		<DataControls xmlns="">
			<Control>
				<Name>RunAction</Name>
				<Type>Button</Type>
				<Methods>
					<Method>
						<Name>clicked</Name>
						<Source><![CDATA[
        /// <summary>
        /// Run processing for current line
        /// </summary>
        public void clicked()
        {
            DialogButton db = Box::yesNo("@SIMSStartProcessing:AreYouSureYouWantToProceed", DialogButton::No);
            
            if (db == DialogButton::No)
            {
                return;
            }

            SIMSStartRunAction  startRunAction = SIMSStartRunAction::construct(SIMSStartTable);
            startRunAction.runOperation();
            SIMSStartTable_ds.research(true);
        }

]]></Source>
					</Method>
				</Methods>
			</Control>
			<Control>
				<Name>ApproveButton</Name>
				<Type>Button</Type>
				<Methods>
					<Method>
						<Name>clicked</Name>
						<Source><![CDATA[
        /// <summary>
        /// Approval button chack - if you are not administartor you can't approve line for yourself. Some kind of difucalties'
        /// </summary>
        public void clicked()
        {
            if (curUserId() == 'Admin' || SIMSStartTable.createdBy != curUserId() || SIMSStartSubAdmins::isUserCanApproveTheLines(curUserId()))
            {
                SIMSStartLog::addLogFromStartTable(SIMSStartTable, strFmt("@SIMSStartProcessing:ApprovedByCurUserId", curUserId()));
                ttsbegin;
                SIMSStartTable.ApprovedBy = curUserId();
                SIMSStartTable.write();
                ttscommit;
                SIMSStartTable_ds.research(true);
            }
            else
            {
                Error("@SIMSStartProcessing:AdminNotOwner");
            }
        }

]]></Source>
					</Method>
