				<Name>enableControlAccess</Name>
				<Source><![CDATA[
    /// <summary>
    /// Enable acces to the dif kind of processing
    /// </summary>
    public void enableControlAccess()
    {
        SIMSStartTable_ds.object(fieldNum(SIMSStartTable, SQL)).allowEdit(SIMSStartTable.TaskType == SIMSStartTaskType::SQL);
        SIMSStartTable_ds.object(fieldNum(SIMSStartTable, AOTClassName)).allowEdit(SIMSStartTable.TaskType == SIMSStartTaskType::Class || SIMSStartTable.TaskType == SIMSStartTaskType::RunBase);
    }

]]></Source>
