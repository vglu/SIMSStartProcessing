	<SourceCode>
		<Declaration><![CDATA[
/// <summary>
/// Main framework table. It store URL/processing and processing rules
/// </summary>
public class SIMSStartTable extends common
{
}
]]></Declaration>
		<Methods>
			<Method>
				<Name>getEnvUrl</Name>
				<Source><![CDATA[
    /// <summary>
    /// Shows URL for current environment
    /// </summary>
    /// <returns>env URL</returns>
    [SysClientCacheDataMethodAttribute(true)]
    public display URL getEnvUrl()
    {
        var applicationEnvironment = Microsoft.Dynamics.ApplicationPlatform.Environment.EnvironmentFactory::GetApplicationEnvironment();
        return applicationEnvironment.Infrastructure.FullyQualifiedDomainName;
    }

]]></Source>
			</Method>
			<Method>
				<Name>initValue</Name>
				<Source><![CDATA[
    public void initValue()
    {
        super();
        this.URLTemplate = this.getEnvUrl();
    }

]]></Source>
			</Method>
			<Method>
				<Name>find</Name>
				<Source><![CDATA[
    /// <summary>
    /// Standard find method
    /// </summary>
    /// <param name = "_refRecId">RecId for record</param>
    /// <param name = "_forupdate">should be record be selected for update</param>
    /// <returns>cursor if found</returns>
    public static SIMSStartTable find(RefRecId _refRecId, boolean _forupdate = false)
    {
        SIMSStartTable  startTable;

        startTable.selectForUpdate(_forupdate);
        if (_refRecId)
        {
            select firstonly firstfast startTable
                where startTable.RecId == _refRecId
                ;
        }

        return startTable;
    }

]]></Source>
			</Method>
			<Method>
				<Name>update</Name>
				<Source><![CDATA[
    public void update()
    {
        if (this.StartAllTime != this.orig().StartAllTime)
        {
            // if Start each time flag changes - we will clear execute field. This importand for understanding if line alreafy fired 
            this.ExecutedOn = dateNull();
        }
        // if we got ANY update (with same approval - let's start appropval process from scratch )
        if (this.ApprovedBy == this.orig().ApprovedBy)
        {
            this.ApprovedBy = '';
        }
        super();
    }

]]></Source>
			</Method>
			<Method>
				<Name>checkIfLineCanRun</Name>
				<Source><![CDATA[
    /// <summary>
    /// Chck if curent line can be run from user interface
    /// </summary>
    /// <returns>true if can be run</returns>
    public boolean checkIfLineCanRun()
    {
        boolean ret = (this.getEnvUrl() like this.URLTemplate) && this.ApprovedBy;

        return ret;
    }

]]></Source>
			</Method>
		</Methods>
