{
}
]]></Declaration>
		<Methods>
			<Method>
				<Name>getEnvUrl</Name>
				<Source><![CDATA[
    [SysClientCacheDataMethodAttribute(true)]
    public display URL getEnvUrl()
    {
        var applicationEnvironment = Microsoft.Dynamics.ApplicationPlatform.Environment.EnvironmentFactory::GetApplicationEnvironment();
        return applicationEnvironment.Infrastructure.FullyQualifiedDomainName;
    }

]]></Source>
			</Method>
			<Method>
				<Name>initValue</Name>
				<Source><![CDATA[
    /// <summary>
    ///
    /// </summary>
    public void initValue()
    {
        super();
        this.URLTemplate = this.getEnvUrl();
    }

]]></Source>
			</Method>
			<Method>
				<Name>find</Name>
				<Source><![CDATA[
    public static SIMSStartTable find(RefRecId _refRecId, boolean _forupdate = false)
    {
        SIMSStartTable  startTable;

        startTable.selectForUpdate(_forupdate);
        if (_refRecId)
        {
            select firstonly firstfast startTable
                where startTable.RecId == _refRecId
                ;
        }

        return startTable;
    }

]]></Source>
			</Method>
			<Method>
				<Name>update</Name>
				<Source><![CDATA[
    /// <summary>
    ///
    /// </summary>
    public void update()
    {
        if (this.StartAllTime != this.orig().StartAllTime)
        {
            this.ExecutedOn = dateNull();
        }
        super();
    }

]]></Source>
			</Method>
			<Method>
				<Name>checkIfLineCanRun</Name>
				<Source><![CDATA[
    public boolean checkIfLineCanRun()
    {
        return (this.getEnvUrl() like this.URLTemplate);
    }

]]></Source>
			</Method>
		</Methods>
