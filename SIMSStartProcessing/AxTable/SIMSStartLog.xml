			<Method>
				<Name>addLogFromStartTable</Name>
				<Source><![CDATA[
    public static void addLogFromStartTable(SIMSStartTable _startTable, str _message)
    {
        SIMSStartLog::addLog(strFmt("URL %1, Description %2, message %3", _startTable.URLTemplate, _startTable.Description, _message));
    }

]]></Source>
			</Method>
			<Method>
				<Name>addLog</Name>
				<Source><![CDATA[
    public static void addLog(str _message)
    {
        SIMSStartLog    startLog;
        startLog.initValue();
        startLog.TransDateTime  = DateTimeUtil::getSystemDateTime();
        startLog.Log            = _message;
        startLog.insert();
    }

]]></Source>
