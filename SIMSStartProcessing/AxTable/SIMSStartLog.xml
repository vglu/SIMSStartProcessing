			<Method>
				<Name>addLogFromStartTable</Name>
				<Source><![CDATA[
    /// <summary>
    /// Add log message for <c>SIMSStartTable</c>
    /// </summary>
    /// <param name = "_startTable">Table itself</param>
    /// <param name = "_message">Message</param>
    public static void addLogFromStartTable(SIMSStartTable _startTable, str _message)
    {
        SIMSStartLog::addLog(strFmt("@SIMSStartProcessing:URLDescriptionMessage", _startTable.URLTemplate, _startTable.Description, _message));
    }

]]></Source>
			</Method>
			<Method>
				<Name>addLog</Name>
				<Source><![CDATA[
    /// <summary>
    /// Add simple log message
    /// </summary>
    /// <param name = "_message">message to save</param>
    public static void addLog(str _message)
    {
        SIMSStartLog    startLog;
        startLog.initValue();
        startLog.TransDateTime  = DateTimeUtil::getSystemDateTime();
        startLog.Log            = _message;
        startLog.insert();
    }

]]></Source>
